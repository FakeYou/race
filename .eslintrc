{
	"extends": "react-app",
	"globals": {
		"__DEV__": true
	},
	"rules": {
		"brace-style": ["error", "stroustrup", { "allowSingleLine": true }],
		"indent": ["warn", "tab", { "SwitchCase": 1 }],
		"linebreak-style": ["warn", "unix"],
		"quotes": ["warn", "single"],
		"semi": ["warn", "always"]
	}
}
